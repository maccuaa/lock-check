{
  "name": "@maccuaa/lock-check",
  "description": "Verify that all the packages in your package-lock.json file exist",
  "version": "3.0.0",
  "author": "Andrew MacCuaig <andrewmaccuaig@gmail.com>",
  "bugs": "https://github.com/maccuaa/lock-check/issues",
  "homepage": "https://github.com/maccuaa/lock-check",
  "repository": "maccuaa/lock-check",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "build": "run-s clean compile",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "async": "3.2.0",
    "cli-ux": "5.5.1",
    "execa": "5.0.0"
  },
  "devDependencies": {
    "@types/async": "3.2.6",
    "@types/node": "16.3.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "lock-check": "./bin/run"
  },
  "preferGlobal": true,
  "files": [
    "/bin",
    "/lib"
  ],
  "keywords": [
    "oclif",
    "artifactory",
    "download"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "oclif": {
    "bin": "lock-check"
  }
}
